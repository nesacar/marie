        "bootstrap": "^4.1.1",
        "browser-sync": "^2.24.6",
        "browser-sync-webpack-plugin": "^2.0.1",
        "concurrently": "^3.5.1",
        "cross-env": "^5.1.6",
        "jquery": "^3.2",
        "laravel-mix": "^2.0",
        "laravel-mix-purgecss": "^2.2.0",
        "lodash": "^4.17.10",
        "mjml": "^4.1.1",
        "npm-run-all": "^4.1.3",
        "popper.js": "^1.14.3",
        "postcss-css-variables": "^0.8.1",
        "vue": "^2.5.17"
    },
    "dependencies": {
        "@casl/ability": "^2.4.2",
        "@casl/vue": "^0.4.2",
        "@fortawesome/fontawesome": "^1.1.8",
        "@fortawesome/fontawesome-free-solid": "^5.0.13",
        "@fortawesome/vue-fontawesome": "0.0.22",
